{
  "name": "rehype-to-text",
  "description": "Plugin that compiles a rehype AST to text using hast-util-to-text",
  "version": "1.0.2",
  "author": "giraugh",
  "license": "MIT",
  "type": "module",
  "main": "index.mjs",
  "types": "index.d.mts",
  "sideEffects": false,
  "keywords": [
    "rehype",
    "hast",
    "text",
    "textContent"
  ],
  "files": [
    "index.mjs",
    "index.d.mts"
  ],
  "scripts": {
    "build": "rimraf '*.d.mts' && tsc"
  },
  "exports": "./index.mjs",
  "dependencies": {
    "hast-util-to-text": "^4.0.0",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
